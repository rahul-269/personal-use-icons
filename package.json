{
  "name": "personal-use-icons",
  "version": "1.0.3",
  "description": "A collection of personal use React/Next.js icons",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "create-icon-components": "node --loader ts-node/esm scripts/generate-components.ts",
    "generate": "node --loader ts-node/esm scripts/generate-index.ts",
    "build": "npm run generate && rollup -c",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "icons",
    "react-icons",
    "svg-icons",
    "nextjs-icons"
  ],
  "author": "rahul-269",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rahul-269/personal-use-icons.git"
  },
  "bugs": {
    "url": "https://github.com/rahul-269/personal-use-icons/issues"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@svgr/rollup": "^8.1.0",
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.10",
    "rollup": "^4.35.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "glob": "^11.0.1"
  }
}
